     pa.`guarantyOrgID`,
     pa.`attributionName`, 
pa.`attributionID`,
     pp.projectName,
     pp.loadSum,
     pp.`subBankName`,
     pp.bankName,
     pa.`fundSource`,
     pp.`delayBeginDate`,
     pp.`delayEndDate`,
     pp.amanName,
     pp.bmanName,
     pfp.`payCapitalSum`,
     pfp.`remark`,
     pfp.`payDate`,
(pp.loadSum - (SELECT IFNULL(SUM(pfp1.payCapitalSum),0) FROM pro_factpay pfp1 WHERE pfp1.project_ID = pfp.project_ID AND pfp1.payDate &lt;= pfp.payDate)) AS 'remainSUM' 
 FROM pro_factpay pfp
 LEFT JOIN pro_project pp ON pfp.project_ID = pp.project_ID
 LEFT JOIN pro_apply pa ON pa.`apply_ID`=pp.`apply_ID`
 WHERE pp.`busiClass`='02' and  pfp.`payCapitalSum` &gt;0
 ${has(begindatesql)?(" and pfp.payDate&gt;='"+begindatesql+"'"):""}
${has(enddatesql)?(" and pfp.payDate&lt;='"+enddatesql+"'"):"AND pfp.payDate&lt;=NOW()"}
ORDER BY pa.attributionName,pfp.`payDate` DESC</Sql><Description>SELECT pa.`guarantyOrgName`,
