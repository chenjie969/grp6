SELECT 
pa.`guarantyOrgName`,
-- pa.`guarantyOrgID`,
pa.`attributionID`,
pa.`oprationCompanyName`,
pa.`hostAreaName`,
pa.`attributionName`,
cc.relationMainName,
pa.`projectName`,
pa.`fundSource`,
pp.`bankName`,
pp.`subBankName`,
pd.`delaySum`,
pp.`delayBeginDate`,
pp.`loadSum`,
SUM((IFNULL(pp.loadSum,0)-IFNULL(tab1.payCapitalSum,0)-IFNULL(tab2.replaceCapitalSum,0)-IFNULL(tab5.creditorSum,0))) AS guarantySum,
pp.`loadEndDate`,
pp.bankRate,
pp.serviceRate,
pd.delayRate,
pd.delayServiceRate,
pd.`delayEndDate`,
pd.`delayMonthDay`,
pa.`amanName`,
pa.`bmanName`,
pp.beforeAManName,
pp.beforeBManName,
pd.`delayReason`,
pa.fundID AS guarantyOrgID,
pa.fundChinese
FROM  pro_delay pd
LEFT JOIN pro_project pp ON pp.project_ID=pd.`project_ID`
LEFT JOIN pro_apply pa ON pa.`apply_ID`=pd.`applyID`
LEFT JOIN crm_client cc ON cc.`client_ID`=pa.`client_ID`
LEFT JOIN (SELECT pfp.project_ID, SUM(IFNULL(pfp.payCapitalSum,0)) AS payCapitalSum         
	FROM pro_factpay pfp WHERE 1=1   GROUP BY pfp.project_ID
)tab1 ON tab1.project_ID =  pp.project_ID
LEFT JOIN ( SELECT pre.project_ID,SUM(IFNULL(pre.replaceCapitalSum,0)) AS replaceCapitalSum         
	FROM pro_replace pre WHERE 1=1   GROUP BY pre.project_ID 
)tab2 ON tab2.project_ID =  pp.project_ID
LEFT JOIN ( SELECT pc.project_id,SUM(IFNULL(pc.creditor_sum,0)) AS creditorSum         
	FROM pro_creditor pc WHERE 1=1   GROUP BY pc.project_id 
)tab5 ON tab5.project_id =  pp.project_ID
 WHERE pp.`busiClass`='02'
${has(begindatesql)?(" and pd.delayBeginDate&gt;='"+begindatesql+"'"):""}
${has(enddatesql)?(" and pd.delayBeginDate&lt;='"+enddatesql+"'"):""}
${has(wheresql)?(" and wheresql='"+wheresql+"'"):""}
${has(dataJurisdictionSql)?(dataJurisdictionSql):""}
ORDER BY pa.`projectName`,pp.`delayBeginDate` ASC
