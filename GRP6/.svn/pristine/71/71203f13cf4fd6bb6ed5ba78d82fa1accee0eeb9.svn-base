<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%>
<div class="modal fade" id="addcreditEnterprise" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" data-backdrop="static" data-keyboard="false">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title" id="myModalLabel">授信企业添加</h4>
      </div>
      <div class="modal-body">
		<form class="form-horizontal" role="form" id="add_form">
			<input type="hidden" name="busisortid" class="ztb_add_busisortid">
			<input type="hidden" name="unitUid" class="ztb_add_unitUid" value="1">
			<input type="hidden" name="pbusisortid" class="ztb_add_pbusisortid">
			<input type="hidden" name="busisortfullcode" class="ztb_add_busisortfullcode" value="1">
			<input type="hidden" name="url" class="ztb_add_url" value="#">
			<input type="hidden" name="orderId" class="ztb_add_orderId" value="1">
			<input type="hidden" name="createUser" class="ztb_add_createUser" value="1">
			<input type="hidden" name="updateUser" class="ztb_add_updateUser" value="1">
			<input type="hidden" name="isdelete" class="ztb_add_isdelete" value="0" >
			<input type="hidden" name="isedit" class="ztb_add_isedit" value="0">
			
			<div class="form-group ">
				<label class="col-sm-4 control-label no-padding-right" for="form-field-1"><i class="icon-asterisk orange"></i>客户名称： </label>
				<div class="col-sm-8">
					<input type="text" name="mod_name" id="add_mod_name"  class="col-xs-10 col-sm-8 ztb_add_mod_name ztb_add validate[required,maxSize[50]]" />
					
				</div>
			</div>
			
			<div class="form-group ">
				<label class="col-sm-4 control-label no-padding-right" for="form-field-2"><i class="icon-asterisk orange"></i>授信生效日期： </label>
				<div class="col-sm-8">
						<div class="row">
							<div class="col-sm-8">
								<div class="input-group">
									<input class="form-control date-picker" id="id-date-picker-1" type="text" data-date-format="yyyy-mm-dd" />
									<span class="input-group-addon">
										<i class="icon-calendar bigger-110"></i>
									</span>
								</div>
							</div>
						</div>
					</div>
			</div>
			
			<div class="form-group ">
				<label class="col-sm-4 control-label no-padding-right" for="form-field-2"><i class="icon-asterisk orange"></i>授信失效日期： </label>
				<div class="col-sm-8">
						<div class="row">
							<div class="col-sm-8">
								<div class="input-group">
									<input class="form-control date-picker" id="id-date-picker-1" type="text" data-date-format="yyyy-mm-dd" />
									<span class="input-group-addon">
										<i class="icon-calendar bigger-110"></i>
									</span>
								</div>
							</div>
						</div>
					</div>
			</div>
			
			<div class="space-4"></div>
			<div class="form-group">
				<label class="col-sm-4 control-label no-padding-right" for="form-field-2"><i class="icon-asterisk orange"></i>授信额度： </label>
				<div class="col-sm-8">
					<input type="text" name="mod_name" id="add_mod_name"  class="col-xs-10 col-sm-8 ztb_add_mod_name ztb_add validate[required,maxSize[50]]" />
				</div>
			</div>
			<div class="form-group ">
				<label class="col-sm-4 control-label no-padding-right" for="form-field-2"><i class="icon-asterisk orange"></i>是否限制业务品种： </label>
				<div class="col-sm-8">
					<input type="radio" name="mod_name" id="add_mod_name"  class="col-xs-3 col-sm-3 ztb_add_mod_name ztb_add validate[required,maxSize[50]]" />是
					
					<input type="radio" name="mod_name" id="add_mod_name"  class="col-xs-3 col-sm-2 ztb_add_mod_name ztb_add validate[required,maxSize[50]]" />否 
			
				</div>
			</div>
	
			<div class="form-group ">
				<label class="col-sm-4 control-label no-padding-right" for="form-field-2"><i class="icon-asterisk orange"></i>授信额度合计（万）： </label>
				<div class="col-sm-8">
					<input type="text" name="mod_name" id="add_mod_name"  class="col-xs-10 col-sm-8 ztb_add_mod_name ztb_add validate[required,maxSize[50]]" />
				</div>
			</div>
			
			<div class="form-group ">
				<label class="col-sm-4 control-label no-padding-right" for="form-field-2"><i class="icon-asterisk orange"></i>经办人： </label>
				<div class="col-sm-8">
					<input type="text" name="mod_name" id="add_mod_name"  class="col-xs-10 col-sm-8 ztb_add_mod_name ztb_add validate[required,maxSize[50]]" />
				</div>
			</div>
		</form>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-primary btn_submit" > <i class='icon-ok bigger-110'></i>保存</button>
        <button type="button" class="btn btn-default" data-dismiss="modal"> <i class='icon-remove bigger-110'></i>关闭</button>
      </div>
    </div>
  </div>
</div>
					