	SUM((IFNULL(pp.loadSum,0)-IFNULL(tab1.payCapitalSum,0)-IFNULL(tab2.replaceCapitalSum,0))) AS beginGuaSum
FROM pro_project pp
LEFT JOIN (
	SELECT 
		pfp.factPay_ID,
		pfp.project_ID,
		SUM(IFNULL(pfp.payCapitalSum,0)) AS payCapitalSum         
	FROM
		pro_factpay pfp
	WHERE 	1=1 
		${has(enddatesql)?(" and pfp.payDate&lt;'"+enddatesql+"'"):""}
	GROUP BY pfp.project_ID
)tab1 ON tab1.project_ID =  pp.project_ID
LEFT JOIN (
