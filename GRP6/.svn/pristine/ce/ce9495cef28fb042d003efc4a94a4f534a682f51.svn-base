<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%>

<%@ include file="/crm/client/companyClient/companyClientHead.jsp"%>
	


<div style="display:none" id="creditInfo_creditInfo_ID"></div>
	<h4 class="header smaller lighter blue">
		申请人及其关联公司在银行授信及使用情况
		<button type="button" name="button" class="btn btn-minier btn-warning pull-right  creditInfo_edit" value="bankCreditInfo">
			<i class="icon-edit bigger-110"></i>
			<span class="bigger-110 no-text-shadow">修改</span>
		</button>
	</h4>
	<div class="row" style="margin:0;">
		<pre id="creditInfo_bankCreditInfo" class="col-xs-12" style="white-space: pre-wrap;"></pre>
	</div>
	<br>
	<h4 class="header smaller lighter blue">
		申请人的历史信用记录
		<button type="button" name="button" class="btn btn-minier btn-warning pull-right  creditInfo_edit" value="historicalCreditRecord">
			<i class="icon-edit bigger-110"></i>
			<span class="bigger-110 no-text-shadow">修改</span>
		</button>
	</h4>
	<br>
	<div class="row" style="margin:0;">
        <h5 class="smaller">未结清贷款余额：<small style="font-size:14px;" id="creditInfo_loanBalance"></small></h5>
	</div>
	<br>
	<div class="row" style="margin:0;">
        <h5 class="smaller">对外担保余额：<small style="font-size:14px;" id="creditInfo_garantySum"></small></h5>
	</div>
	<br>
	<div class="row" style="margin:0;">
        <h5 class="smaller">其它重要事项：</h5>
        <pre id="creditInfo_otherDesc" class="col-xs-12" style="white-space: pre-wrap;"></pre>
	</div>
    <br>
	<h4 class="header smaller lighter blue">
		人民银行信贷征信系统查询结果
		<button type="button" name="button" class="btn btn-minier btn-warning pull-right  creditInfo_edit" value="creditResult">
			<i class="icon-edit bigger-110"></i>
			<span class="bigger-110 no-text-shadow">修改</span>
		</button>
	</h4>
	<div class="row" style="margin:0;">
		 <pre id="creditInfo_creditResult" class="col-xs-12" style="white-space: pre-wrap;"></pre>
	</div>
	<br>
	<h4 class="header smaller lighter blue">
		申请人及其关联人、关联企业是否涉及诉讼、仲裁及执行情况等法律文件
		<button type="button" name="button" class="btn btn-minier btn-warning pull-right creditInfo_edit" value="lawsuitInfo">
			<i class="icon-edit bigger-110"></i>
			<span class="bigger-110 no-text-shadow">修改</span>
		</button>
	</h4>
	<div class="row" style="margin:0;">
	    <pre id="creditInfo_lawsuitInfo" class="col-xs-12" style="white-space: pre-wrap;"></pre>
	</div>
	<br>
	<h4 class="header smaller lighter blue">
		市场软消息（同行业评价、银行评价、周边熟悉人评价等）
		<button type="button" name="button" class="btn btn-minier btn-warning pull-right creditInfo_edit" value="marketMessage">
			<i class="icon-edit bigger-110"></i>
			<span class="bigger-110 no-text-shadow">修改</span>
		</button>
	</h4>
	<div class="row" style="margin:0;">
	    <pre id="creditInfo_marketMessage" class="col-xs-12" style="white-space: pre-wrap;"></pre>
	</div>

<script src="<%=path %>/crm/client/creditInfo/creditInfo.js?v=<%=vardate%>"></script>
