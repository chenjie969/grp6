 * 角色与数据授权关系实体
 */
public class Sys_role_data {
	
	private Long id;//主键id       
	private String role_uid;//角色id             
	private int operation_type;//操作类型 
	private int accredit_type;//业务类型
	private String unit_uid;//担保机构id
	
	public Long getId() {
		return id;
	}
	public void setId(Long id) {
		this.id = id;
	}
	public String getRole_uid() {
		return role_uid;
	}
	public void setRole_uid(String role_uid) {
		this.role_uid = role_uid;
	}
	public int getOperation_type() {
		return operation_type;
	}
	public void setOperation_type(int operation_type) {
		this.operation_type = operation_type;
	}
	public int getAccredit_type() {
		return accredit_type;
	}
	public void setAccredit_type(int accredit_type) {
		this.accredit_type = accredit_type;
	}
	public String getUnit_uid() {
		return unit_uid;
	}
	public void setUnit_uid(String unit_uid) {
		this.unit_uid = unit_uid;
	}
	
	
	
}
