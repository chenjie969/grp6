	function initData(){
		//关联诉讼信息 展示关联的查询信息id
		$("input[type=radio][name=isConSuit]").change(function(){
			if($(this).val() == '1') {
				$("#div-lawSuit").show();
				$("#div-assetSealUp").hide();
				$("#assetSealId").val("");
				$("#assetSealName").val("");
			} else if($(this).val() == '2') {
				$("#div-lawSuit").hide();
				$("#div-assetSealUp").show();
				$("#lawSuitId").val("");
				$("#lawSuitName").val("");
			} else {
				$("#div-lawSuit").hide();
				$("#div-assetSealUp").hide();
				$("#lawSuitId").val("");
				$("#lawSuitName").val("");
				$("#assetSealId").val("");
				$("#assetSealName").val("");
			}
		});
		
		//初始化关联关系
		if($("input[type=radio][name=isConSuit]:checked").val() == '1'){
			$("#div-lawSuit").show();
			$("#div-assetSealUp").hide();
		}else if($("input[type=radio][name=isConSuit]:checked").val() == '2'){
			$("#div-lawSuit").hide();
			$("#div-assetSealUp").show();
		} else {
			$("#div-lawSuit").hide();
			$("#div-assetSealUp").hide();
		}
		
		//融投作为破产债务人显示资产划扣情况
		$("input[type=radio][name=obligorType]").change(function(){
			if($(this).val() == '01') {
				$("#div-fundDeduction").show();
			} else {
				$("#div-fundDeduction").hide();
			}
		});
		
		
	}
	
