FROM pro_project  fl
LEFT JOIN
(SELECT fd.project_ID,SUM(IFNULL(fd.payCapitalSum,0)) AS freeSum FROM pro_factPay fd 
where 1=1
 ${has(enddatesql)?(" and fd.payDate&lt;'"+enddatesql+"'"):"and fd.payDate &lt; now()"}
GROUP BY fd.project_ID
) tab ON tab.`project_ID` = fl.`project_ID`
LEFT JOIN
(select pr.project_ID,sum(IFNULL(pr.replaceCapitalSum,0)) as replaceSum from pro_replace pr 
where 1=1
 ${has(enddatesql)?(" and pr.replaceDate&lt;'"+enddatesql+"'"):"and pr.replaceDate&lt;now()"}
GROUP BY pr.project_ID ) tab2 on tab2.project_ID = fl.project_ID
LEFT JOIN (
	SELECT
		 precre.project_ID,
		SUM(IFNULL(precre.creditor_sum,0)) AS creditorsum 
	FROM 
		pro_creditor precre
    	WHERE 1=1 
		${has(enddatesql)?(" and DATE_FORMAT(precre.creditor_date,'%Y-%m')&lt;'"+enddatesql+"'"):""}
	GROUP BY precre.project_ID
   )tab5 ON tab5.project_ID =  fl.project_ID

WHERE  fl.busiClass = '01'
${has(enddatesql)?(" and fl.reportDate &lt;'"+enddatesql+"'"):""}
</Sql><Description>SELECT  (IFNULL(fl.loadSum,0)-IFNULL(tab.freeSum,0)-IFNULL(tab2.replaceSum,0)-IFNULL(tab5.creditorsum,0)) AS guarantySum
