dren ItemClass="DatasetNodeSource"><Item0><Children ItemClass="RowNodeSource"><Item0/></Children><Reader Class="SqlReader"><Name>tab_detail</Name><Sql>
 SELECT  
	pa.guarantyOrgName,c.clientName,SUM(IFNULL(p.loadSum,0)) as loadSum,p.bankName,c.gzwRiskLevel
from pro_project p
LEFT JOIN pro_apply pa on pa.apply_ID = p.apply_ID 
LEFT JOIN crm_client c on c.client_ID = pa.client_ID
WHERE 1=1 and p.isGuaranty=0  and p.busiClass='02'
${has(begindatesql)?(" and p.reportDate&gt;='"+begindatesql+"'"):""}
${has(enddatesql)?(" and p.reportDate&lt;='"+enddatesql+"'"):" and p.reportDate&lt;=NOW()"}
GROUP BY p.project_ID
ORDER BY pa.guarantyOrgName desc</Sql><Description>
