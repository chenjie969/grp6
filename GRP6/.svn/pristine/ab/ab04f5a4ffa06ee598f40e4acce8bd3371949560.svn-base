<!-- 新增一个项目诉讼 -->
	<insert id="insertOneProjectLawsuit" parameterType="com.zjm.pro.db.model.Pro_projectLawsuit">
		<![CDATA[
		INSERT INTO Pro_projectLawsuit (
		        projectLawsuit_ID,		
		        assetSealUp_ID,	
		        recordNum,
		        plaintiff,
		        defendant,
				lawsuitSum,				
				lawsuitDate,		
				lawCourt,
				caseInfo,
				propertyInfo,
				unit_uid,
				caseType,
				dicTypeID,
				dicTypeName,
		        effectiveLegalDocuments,
				firstSeal,				
				waiting,		
				undertakeJudge,
				plaintiffApplyExecute,
				executionBasisNum,
				executionBasisType,
				province,
				ifSentenceIdentical,
				ifReview,
		        otherPartyType,
				ifWorkingGroup,				
				remark,	
				projectIDList,
				projectNameList,
				fundDeduction,
				lawsuitType,
				affiliateGroup,
				assetSealUpName,
				createUserName,
				createDateTime,
				updateUserName,			
				updateDateTime
		)
		VALUES
		(
				#{projectLawsuit_ID},
				#{assetSealUp_ID},
				#{recordNum},
				#{plaintiff},
				#{defendant},				
				#{lawsuitSum},				
				#{lawsuitDate},				
				#{lawCourt},
				#{caseInfo},
				#{propertyInfo},
				#{unit_uid},	
				#{caseType},
				#{dicTypeID},
				#{dicTypeName},				
				#{effectiveLegalDocuments},				
				#{firstSeal},
				#{waiting},
				#{undertakeJudge},
				#{plaintiffApplyExecute},	
				#{executionBasisNum},
				#{executionBasisType},			
				#{province},				
				#{ifSentenceIdentical},
				#{ifReview},
				#{otherPartyType},
				#{ifWorkingGroup},
				#{remark},	
				#{projectIDList},	
				#{projectNameList},	
				#{fundDeduction},	
				#{lawsuitType},
				#{affiliateGroup},
				#{assetSealUpName},
				#{createUserName},
				NOW(),
				#{updateUserName},		
				NOW()			
		) ;
		]]>
	</insert>		
