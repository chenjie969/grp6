SELECT pa.`attributionName`,
pa.`guarantyOrgID`,
pa.`guarantyOrgName`,
pa.`projectName`,
pp.`bankName`,
pp.`subBankName`,
pd.`delaySum`,
pp.`delayBeginDate`,
pp.`loadSum`,
pp.`loadEndDate`,
pd.`delayEndDate`,
pd.`delayMonthDay`,
pa.`amanName`,
pa.`bmanName`,
pd.`delayReason`
FROM  pro_delay pd
LEFT JOIN pro_project pp ON pp.project_ID=pd.`project_ID`
LEFT JOIN pro_apply pa ON pa.`apply_ID`=pd.`applyID`
 WHERE pp.`busiClass`='02'
${has(enddatesql)?(" and pd.delayendDate&lt;='"+enddatesql+"'"):""}
${has(wheresql)?(" and wheresql='"+wheresql+"'"):""}
ORDER BY pa.`attributionName`,pp.`delayBeginDate` DESC



