SELECT  pa.`guarantyOrgName`,
pa.`guarantyOrgID`,
     pa.`attributionName`,
     pp.projectName,
     pp.loadSum,
     pp.`delayBeginDate`,
     pp.`delayEndDate`,
     pp.`bankName`,
     pp.`subBankName`,
     pp.`InterestMethodName`,
     pad.`dcontractCode`,
     pp.bankRate,
     pp.serviceRate,
     pp.`amanName`,
     pp.`bmanName` ,
     pp.beforeAManName,
     tab3.disposeRemark,
     pp.beforeBManName
 FROM pro_project pp
 LEFT JOIN pro_apply pa ON pa.`apply_ID`=pp.`apply_ID`
 LEFT JOIN pro_applydetail pad ON pad.`applyDetail_ID` = pp.`applyDetail_ID`
 LEFT JOIN (SELECT GROUP_CONCAT(po.disposeRemark) disposeRemark,po.apply_ID FROM pro_optguaranty po 
 WHERE 1=1 GROUP BY po.apply_ID) tab3 ON tab3.apply_ID = pp.`apply_ID`
 WHERE pp.`busiClass`='02' 
${has(begindatesql)?(" and pp.reportDate&gt;='"+begindatesql+"'"):""}
${has(enddatesql)?(" and pp.reportDate&lt;='"+enddatesql+"'"):""}
${has(wheresql)?(" and wheresql='"+wheresql+"'"):""} 
${has(dataJurisdictionSql)?(dataJurisdictionSql):""}
ORDER BY     pa.`attributionName`,pp.`delayBeginDate` desc



