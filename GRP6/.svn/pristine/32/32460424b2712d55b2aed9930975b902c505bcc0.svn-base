Field>guarantyOrgID</Field></Group></Item0></Children><Reader Class="SqlReader"><Name>tab_month_detail_entrust_factLoad</Name><Sql>-- sql start runing ..
SELECT  pa.`guarantyOrgName`,
pa.`attributionID`,
pa.`guarantyOrgID`,
     pa.`attributionName`,
     pp.projectName,
     pp.loadSum,
     pp.`delayBeginDate`,
     pp.`delayEndDate`,
     pp.`bankName`,
     pp.`subBankName`,
     pp.`InterestMethodName`,
     pad.`dcontractCode`,
     pp.`amanName`,
     pp.`bmanName`  
 FROM pro_project pp
 LEFT JOIN pro_apply pa ON pa.`apply_ID`=pp.`apply_ID`
 LEFT JOIN pro_applydetail pad ON pad.`applyDetail_ID` = pp.`applyDetail_ID`
 WHERE pp.`busiClass`='02' 
${has(begindatesql)?(" and pp.delayBeginDate&gt;='"+begindatesql+"'"):""}
${has(enddatesql)?(" and pp.delayBeginDate&lt;='"+enddatesql+"'"):""}
${has(wheresql)?(" and wheresql='"+wheresql+"'"):""} 
ORDER BY  pa.`attributionName`,pp.`delayBeginDate` DESC



