SELECT 

	project.bankName,
	project.busiTypeName,
	IFNULL(project.loadSum,0) AS loadSum,
	delay.delayMonthDay,
	project.loadBeginDate,
	project.loadEndDate,
	IFNULL(delay.delaySum,0) AS deplaySum,
	delay.delayEndDate,
	delay.delayRate,
	delay.delayServiceRate,
	applyDetail.AreaCodeName,
	applyDetail.projectSourceName,
	applyDetail.amanName,
	applyDetail.bmanName,
	apply.projectName
	

 FROM pro_delay delay
 LEFT JOIN pro_project project ON project.project_ID = delay.project_ID
 LEFT JOIN pro_applydetail applyDetail ON applyDetail.applyDetail_ID = project.applyDetail_ID
 LEFT JOIN pro_apply apply ON apply.apply_ID = project.apply_ID
 WHERE 1=1 

${has(begindatesql)?(" and delay.delayBeginDate&gt;='"+begindatesql+"'"):""}
${has(enddatesql)?(" and delay.delayBeginDate&lt;='"+enddatesql+"'"):"and delay.delayBeginDate&lt;=CURDATE()"}


ORDER BY applyDetail.AreaCodeName

