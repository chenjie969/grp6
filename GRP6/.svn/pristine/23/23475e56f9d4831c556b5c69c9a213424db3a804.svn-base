<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%>
<div class="modal fade" id="MachineView" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" data-backdrop="static" data-keyboard="false">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title" id="myModalLabel">查看机器设备及车辆等</h4>
      </div>
      <div class="modal-body">
      		<div class="row col-sm-offset-1">
				<h5 class="col-sm-11">财产名称：<span class="grey ztb_view_assetsName"></span></h5>
				<div class="space-4"></div>
				<h5 class="col-sm-11">购入时间： <span class="grey ztb_view_buyDate"></span></h5>
				<div class="space-4"></div>
				<h5 class="col-sm-11">有无发票：<span class="grey ztb_view_isBill"></span></h5>
				<div class="space-4"></div>
				<h5 class="col-sm-11">原价：<span class="grey ztb_view_oldValue"></span>&nbsp;万元</h5>
				<div class="space-4"></div>
				<h5 class="col-sm-11">余值：<span class="grey ztb_view_residualValue"></span>&nbsp;万元</h5>
				<div class="space-4"></div>
				<h5 class="col-sm-11">状态：<span class="grey ztb_view_status"></span></h5>
				<div class="space-4"></div>
				<h5 class="col-sm-11">贷款行：<span class="grey ztb_view_loanBank"></span></h5>
				<div class="space-4"></div>
				<h5 class="col-sm-11">抵押金额：<span class="grey ztb_view_guarantySum"></span>&nbsp;万元</h5>
				<div class="space-4"></div>
				<h5 class="col-sm-11">备注：<span class="grey ztb_view_remark"></span></h5>
			</div>	
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal"> <i class='icon-remove bigger-110'></i>关闭</button>
      </div>
    </div>
  </div>
</div>
				