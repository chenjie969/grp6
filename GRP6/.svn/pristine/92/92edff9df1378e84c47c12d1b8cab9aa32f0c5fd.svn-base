   ifnull(pp.loadSum,0) as loadSum,
   cc.`relationMainName`,
   cc.fullAreaName,
   pp.bankName,
   pp.subBankName,
   pa.fundChinese,
   pa.`fundName`,
   cc.personNum AS idCard,
   pp.creditorDesc
   
FROM pro_project pp 
LEFT JOIN pro_apply pa  ON pp.apply_ID = pa.apply_ID
LEFT JOIN crm_client cc ON cc.client_ID = pa.client_ID
where 1=1   and  pp.isCreditor = 1
${has(enddatesql)?(" and pp.delayBeginDate&lt;='"+enddatesql+"'"):""}
${has(dataJurisdictionSql)?(dataJurisdictionSql):""}
