package com.zjm.oa.db.model;

import java.io.Serializable;
import java.util.Date;

public class Hr_staffRewards implements Serializable {
	private String rewardsID; // rewardsID
	private String staffcase_Id;
	private String rewardsType; // 奖赏类型(rewardsType)
	private String rewardsResults; // 奖赏结果(rewardsResults)
	private String rewardsReason; // 奖赏事由(rewardsReason)
	private Date rewardsDate; // 奖赏日期(rewardsDate)
	private String rewardsNotes; // 奖赏备注(rewardsNotes)
	public String getRewardsID() {
		return rewardsID;
	}
	public void setRewardsID(String rewardsID) {
		this.rewardsID = rewardsID;
	}
	public String getStaffcase_Id() {
		return staffcase_Id;
	}
	public void setStaffcase_Id(String staffcase_Id) {
		this.staffcase_Id = staffcase_Id;
	}
	public String getRewardsType() {
		return rewardsType;
	}
	public void setRewardsType(String rewardsType) {
		this.rewardsType = rewardsType;
	}
	public String getRewardsResults() {
		return rewardsResults;
	}
	public void setRewardsResults(String rewardsResults) {
		this.rewardsResults = rewardsResults;
	}
	public String getRewardsReason() {
		return rewardsReason;
	}
	public void setRewardsReason(String rewardsReason) {
		this.rewardsReason = rewardsReason;
	}
	public Date getRewardsDate() {
		return rewardsDate;
	}
	public void setRewardsDate(Date rewardsDate) {
		this.rewardsDate = rewardsDate;
	}
	public String getRewardsNotes() {
		return rewardsNotes;
	}
	public void setRewardsNotes(String rewardsNotes) {
		this.rewardsNotes = rewardsNotes;
	}

	
}
