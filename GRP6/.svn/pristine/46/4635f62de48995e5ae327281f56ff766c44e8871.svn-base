   p.oprationCompanyID,
	 p.oprationCompanyName,
	 p.projectName,
   ifnull( fd.payCapitalSum,0) as freeSum,
	 fl.busiClass,
	fl.bankName,
   fl.subBankName,
   p.fundSource,
   fl.delayBeginDate,
   fl.delayEndDate,
   p.amanID,
   p.bmanID,
	 fd.payDate
FROM pro_factPay fd
LEFT JOIN pro_apply p ON fd.applyID=p.apply_ID
LEFT JOIN pro_project fl ON fl.project_ID=fd.project_ID
WHERE 
