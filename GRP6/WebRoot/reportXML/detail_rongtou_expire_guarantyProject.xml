PageFormat></Page><NodeSource><Children ItemClass="DatasetNodeSource"><Item0><Children ItemClass="RowNodeSource"><Item0/></Children><Reader Class="SqlReader"><Name>tab_detail_factload</Name><Sql>SELECT  
	pa.oprationCompanyID,pa.oprationCompanyName,
 	pa.guarantyOrgID,pa.guarantyOrgName,
	pa.attributionID,pa.attributionName,
	p.projectName,SUM(IFNULL(p.loadSum,0)) as loadSum,p.bankName,
	pa.fundSource,p.delayBeginDate,p.delayEndDate,
	p.amanID,p.bmanID
 from pro_project p
LEFT JOIN pro_apply pa on pa.apply_ID=p.apply_ID
LEFT JOIN pro_applydetail paa on paa.apply_ID=pa.apply_ID
where 1=1 
${has(begindatesql)?(" and p.delayBeginDate &gt;='"+begindatesql+"'"):""}
${has(enddatesql)?(" and p.delayEndDate &lt;='"+enddatesql+"'"):""}
GROUP BY p.project_ID</Sql><Description>SELECT  
