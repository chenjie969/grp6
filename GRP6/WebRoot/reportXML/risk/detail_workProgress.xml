prs.lawsuitInfo,prs.lawsuitProgress
FROM (SELECT riskScheme_ID,createDate FROM pro_riskScheme WHERE STATUS = '审批通过' ${has(enddatesql)?(" and finishDate&lt;='"+enddatesql+"'"):""} GROUP BY relationMain_ID ORDER BY createDate DESC) tab
LEFT JOIN pro_riskScheme prs ON prs.riskScheme_ID = tab.riskScheme_ID
LEFT JOIN crm_relationmain cr ON prs.`relationMain_ID` = cr.`relationMain_ID`
LEFT JOIN crm_client cc ON cr.`clientID` = cc.`client_ID`
LEFT JOIN (SELECT pa.clientGUID,pa.guarantyOrgName FROM pro_apply pa GROUP BY pa.clientGUID) tab1 ON tab1.clientGUID = cc.clientGUID
ORDER BY tab.createDate DESC
</Sql><Description>SELECT prs.relationMainName,tab1.guarantyOrgName,prs.workProgress,tab.createDate,prs.nextPlan,
