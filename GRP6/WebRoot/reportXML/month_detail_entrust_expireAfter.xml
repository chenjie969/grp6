pa.`guarantyOrgID`,
pa.`guarantyOrgName`,
     pa.`oprationCompanyName`,
     pa.`hostAreaName`,
     pa.`attributionName`,
     pp.projectName,
     pp.loadSum,
     pp.`bankName`,
     pp.`subBankName`,
     pp.`delayBeginDate`,
     pp.`delayEndDate`,
    -- pp.`overSum`,
  pp.loadSum-
        (SELECT IFNULL(SUM(replaceCapitalSum) ,0) replaceCapitalSum from pro_replace WHERE project_ID=pp.project_ID ${has(endyearmonthsql)?("and  DATE_FORMAT(replaceDate,'%Y-%m') &lt;='"+endyearmonthsql+"'"):""}  ) -
        (SELECT IFNULL(SUM(payCapitalSum) ,0) payCapitalSum from pro_factpay where  project_ID=pp.project_ID ${has(endyearmonthsql)?("and  DATE_FORMAT(payDate,'%Y-%m') &lt;='"+endyearmonthsql+"'"):""}  ) -
        (SELECT IFNULL(SUM(creditor_sum) ,0) creditorSum from pro_creditor where  project_ID=pp.project_ID ${has(endyearmonthsql)?("and  DATE_FORMAT(creditor_date,'%Y-%m') &lt;='"+endyearmonthsql+"'"):""}  ) as overSum,
    ( IFNULL(pp.interest,0)+
                IFNULL((pp.loadSum-
        (SELECT IFNULL(SUM(replaceCapitalSum) ,0) replaceCapitalSum from pro_replace WHERE project_ID=pp.project_ID ${has(endyearmonthsql)?("and  DATE_FORMAT(replaceDate,'%Y-%m') &lt;='"+endyearmonthsql+"'"):""}  ) -
        (SELECT IFNULL(SUM(payCapitalSum) ,0) payCapitalSum from pro_factpay where  project_ID=pp.project_ID ${has(endyearmonthsql)?("and  DATE_FORMAT(payDate,'%Y-%m') &lt;='"+endyearmonthsql+"'"):""}  ) -
        (SELECT IFNULL(SUM(creditor_sum) ,0) creditorSum from pro_creditor where  project_ID=pp.project_ID ${has(endyearmonthsql)?("and  DATE_FORMAT(creditor_date,'%Y-%m') &lt;='"+endyearmonthsql+"'"):""}  ))*punishRate/100/360*(TIMESTAMPDIFF(DAY,pp.loadEndDate, ${has(endyearmonthsql)?(endyearmonthsql):"now()"})+1),0)+
IFNULL(pp.interest*pp.punishRate/100/360*(TIMESTAMPDIFF(DAY,pp.loadBeginDate, ${has(endyearmonthsql)?(endyearmonthsql):"now()"})+1),0)-
(SELECT IFNULL(SUM(payInterestSum) ,0)  from pro_factpay where  project_ID=pp.project_ID  ${has(endyearmonthsql)?("and  DATE_FORMAT(payDate,'%Y-%m') &lt;='"+endyearmonthsql+"'"):""} )) as interestN,
(tab1.interest+IFNULL((pp.loadSum-
        (SELECT IFNULL(SUM(replaceCapitalSum) ,0) replaceCapitalSum from pro_replace WHERE project_ID=pp.project_ID  ${has(endyearmonthsql)?("and  DATE_FORMAT(replaceDate,'%Y-%m') &lt;='"+endyearmonthsql+"'"):""} ) -
        (SELECT IFNULL(SUM(payCapitalSum) ,0) payCapitalSum from pro_factpay where  project_ID=pp.project_ID  ${has(endyearmonthsql)?("and  DATE_FORMAT(payDate,'%Y-%m') &lt;='"+endyearmonthsql+"'"):""} ) -
        (SELECT IFNULL(SUM(creditor_sum) ,0) creditorSum from pro_creditor where  project_ID=pp.project_ID  ${has(endyearmonthsql)?("and  DATE_FORMAT(creditor_date,'%Y-%m') &lt;='"+endyearmonthsql+"'"):""} ))*pp.punishRate/100/360*(TIMESTAMPDIFF(DAY,tab1.factDate, ${has(endyearmonthsql)?(endyearmonthsql):"now()"})+1),0)+
IFNULL(pp.interest*pp.punishRate/100/360*(TIMESTAMPDIFF(DAY,tab1.factDate, now())+1),0)-
(SELECT IFNULL(SUM(payInterestSum) ,0)  from pro_factpay where  project_ID=pp.project_ID ${has(endyearmonthsql)?("and  DATE_FORMAT(payDate,'%Y-%m') &lt;='"+endyearmonthsql+"'"):""} )) AS interestY,
     pp.`overDesc`,
     pp.`amanName`,
     pp.`bmanName`,
     pp.beforeAManName,
     pp.beforeBManName,
     pp.bankRate,
     pp.serviceRate,
     cc.relationMainName,
     tab3.disposeRemark
 FROM pro_project pp
 LEFT JOIN pro_apply pa ON pa.`apply_ID`=pp.`apply_ID`
 LEFT JOIN crm_client cc ON cc.`client_ID`=pa.`client_ID`
 LEFT JOIN (SELECT GROUP_CONCAT(po.disposeRemark) disposeRemark,po.apply_ID FROM pro_optguaranty po
 WHERE 1=1 GROUP BY po.apply_ID) tab3 ON tab3.apply_ID = pp.`apply_ID`
        LEFT JOIN (SELECT MAX(factDate) AS factDate,project_ID,interest FROM pro_factpay
    WHERE 1=1  ${has(endyearmonthsql)?("and  DATE_FORMAT(payDate,'%Y-%m') &lt;='"+endyearmonthsql+"'"):""} GROUP BY project_ID
  )tab1 ON tab1.project_ID =  pp.project_ID
 WHERE pp.`busiClass`='02' 
-- and pp.isOver = 1
 AND ( (pp.isOver = 1  ${has(endyearmonthsql)?(" and pp.overDate&lt;='"+endyearmonthsql+"'"):""} ) or (pp.finishDate is NULL and ${has(endyearmonthsql)?("  pp.delayEndDate&lt;='"+endyearmonthsql+"'"):"pp.delayEndDate&lt;=now()"}) or pp.finishDate&gt; pp.overDate)
-- ${has(endyearmonthsql)?(" and pp.overDate&lt;='"+endyearmonthsql+"'"):""}
 ${has(endyearmonthsql)?(" and pp.reportDate&lt;='"+endyearmonthsql+"'"):""}
${has(wheresql)?(" and wheresql='"+wheresql+"'"):""}
${has(dataJurisdictionSql)?(dataJurisdictionSql):""}
ORDER BY pa.attributionName,pp.`delayBeginDate` desc</Sql><Description>SELECT
