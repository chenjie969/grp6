(select 
count(*) as tab1_count,
IFNULL(sum(lawsuit_sum),0) as tab1_sum
from pro_executecase
where plaintiff =  '河北融投担保集团有限公司' and province = '省内' 
${has(enddatesql)?(" and lawsuit_date&lt;='"+enddatesql+"'"):""}
) tab JOIN
(select 
count(*) as tab2_count,
IFNULL(sum(lawsuit_sum),0) as tab2_sum
from pro_executecase
where plaintiff =  '河北融投担保集团有限公司' and province = '省外'
${has(enddatesql)?(" and lawsuit_date&lt;='"+enddatesql+"'"):""}
) tab2 JOIN
(select 
count(*) as tab3_count,
IFNULL(sum(lawsuit_sum),0) as tab3_sum
from pro_executecase
where defendant =  '河北融投担保集团有限公司' and province = '省内'
${has(enddatesql)?(" and lawsuit_date&lt;='"+enddatesql+"'"):""}
) tab3 JOIN
(select 
count(*) as tab4_count,
IFNULL(sum(lawsuit_sum),0) as tab4_sum
from pro_executecase
where defendant =  '河北融投担保集团有限公司' and province = '省外'
${has(enddatesql)?(" and lawsuit_date&lt;='"+enddatesql+"'"):""}
) tab4 JOIN
(select 
count(*) as tab5_count,
IFNULL(sum(lawsuit_sum),0) as tab5_sum
from pro_executecase
where defendant =  '河北融投担保集团有限公司' 
${has(enddatesql)?(" and PERIOD_DIFF( date_format('"+enddatesql+"' , '%Y%m' ) , date_format( lawsuit_date, '%Y%m' ) ) =1 "):""}
) tab5 JOIN
(select 
count(*) as tab6_count,
IFNULL(sum(lawsuit_sum),0) as tab6_sum
from pro_executecase
where plaintiff =  '河北融投担保集团有限公司' 
${has(enddatesql)?(" and PERIOD_DIFF( date_format('"+enddatesql+"' , '%Y%m' ) , date_format( lawsuit_date, '%Y%m' ) ) =1 "):""}
) tab6









