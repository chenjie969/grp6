Style>1</Style><Size>12</Size></Font><HorizontalAlignment>1</HorizontalAlignment><Format Class="DecimalFormat"><Pattern>#,##0.000000</Pattern></Format><Wordwrap>true</Wordwrap><PrintStyle>auto-size:height;</PrintStyle><Cell><Row>3</Row><Col>6</Col></Cell></Item32><Item33 Class="Text"><Variable>="共"+$.clientGUID.distinctCount()+"户"+$.project_ID.count()+"笔业务"</Variable><Border><StyleText>border:1.0px solid #000000</StyleText></Border><Font><Face>宋体</Face><Style>1</Style><Size>12</Size></Font><HorizontalAlignment>1</HorizontalAlignment><Wordwrap>true</Wordwrap><Cell><Row>3</Row><Col>7</Col></Cell></Item33><Item34 Class="Label"><Text>担保集团业务总计</Text><Border><StyleText>border:1.0px solid #000000</StyleText></Border><Font><Face>宋体</Face><Style>1</Style><Size>12</Size></Font><HorizontalAlignment>1</HorizontalAlignment><Wordwrap>true</Wordwrap><PrintStyle>auto-size:height;</PrintStyle><Cell><Row>3</Row><Col>1</Col><ColSpan>5</ColSpan></Cell></Item34></Children><ColumnWidths>60,80,80,80,80,267,127,108,88,88,211,81,114,78,195,172,188,60,60</ColumnWidths><RowHeights>30,30,30,30</RowHeights></Item0></Children><Type>102</Type></Item2></Children><PageFormat><Width>2314</Width><Height>1122</Height><Left>0</Left><Right>8</Right></PageFormat></Page><NodeSource><Children ItemClass="DatasetNodeSource"><Item0><Children ItemClass="GroupNodeSource"><Item0><Children ItemClass="RowNodeSource"><Item0/></Children><Group><Field>oprationCompanyID</Field></Group></Item0></Children><Reader Class="SqlReader"><Name>tab_month_detail_entrust_after</Name><Sql>SELECT  
    pa.`guarantyOrgID`,
    pa.`guarantyOrgName`,
     pa.`oprationCompanyName`,
     pa.oprationCompanyID,
     pa.`hostAreaName`,
     pa.`attributionName`, 
     pp.projectName,
     pp.loadSum - IFNULL(tab1.payCapitalSum,0)-IFNULL(tab2.replaceCapitalSum,0)-IFNULL(tab5.creditorsum,0) AS `guarantySum`, 
     pp.`bankName`,
     pp.`subBankName`,
     pa.`fundSource`,
     pp.`delayBeginDate`,
     pp.`delayEndDate`,
     pp.`InterestMethodName`,
     pad.`dcontractCode`,
     pp.`amanName`,
     pp.`bmanName`,
     pp.beforeAManName,
     pp.beforeBManName,
     cc.relationMainName,
     cc.`clientGUID`,
     pp.bankRate,
     pp.serviceRate,
     pp.punishRate,
     pp.`project_ID`,
    tab3.disposeRemark  
 FROM pro_project pp
 LEFT JOIN pro_apply pa ON pa.`apply_ID`=pp.`apply_ID`
 LEFT JOIN crm_client cc ON cc.`client_ID`=pa.`client_ID`
 LEFT JOIN pro_applydetail pad ON pad.`applyDetail_ID` = pp.`applyDetail_ID`
 LEFT JOIN (SELECT pfp.project_ID, SUM(IFNULL(pfp.payCapitalSum,0)) AS payCapitalSum FROM pro_factpay pfp
    WHERE 1=1 ${has(endyearmonthsql)?(" and  DATE_FORMAT(pfp.payDate,'%Y-%m')&lt;='"+endyearmonthsql+"'"):""} GROUP BY pfp.project_ID
  )tab1 ON tab1.project_ID =  pp.project_ID
 LEFT JOIN (SELECT pre.project_ID,SUM(IFNULL(pre.replaceCapitalSum,0)) AS replaceCapitalSum FROM pro_replace pre
    WHERE 1=1 ${has(endyearmonthsql)?(" and  DATE_FORMAT(pre.replaceDate,'%Y-%m')&lt;='"+endyearmonthsql+"'"):""} GROUP BY pre.project_ID
   )tab2 ON tab2.project_ID =  pp.project_ID 
 LEFT JOIN (SELECT precre.project_ID,SUM(IFNULL(precre.creditor_sum,0)) AS creditorsum FROM pro_creditor precre
   WHERE 1=1 ${has(endyearmonthsql)?(" and precre.creditor_sum &lt;='"+endyearmonthsql+"'"):""} GROUP BY precre.project_ID
   )tab5 ON tab5.project_ID =  pp.project_ID 
 LEFT JOIN (SELECT CONCAT(po.disposeRemark) AS disposeRemark,po.apply_ID FROM pro_optguaranty po WHERE 1=1 GROUP BY po.apply_ID) tab3 ON tab3.apply_ID = pp.`apply_ID`
 WHERE pp.`busiClass`='02' 
-- AND pp.isFree = 0  原条件
 AND ( pp.finishDate is  null  ${has(endyearmonthsql)?(" or DATE_FORMAT(pp.finishDate,'%Y-%m') &gt; '"+endyearmonthsql+"'"):""}  )
${has(endyearmonthsql)?(" and DATE_FORMAT(pp.reportDate,'%Y-%m') &lt;= '"+endyearmonthsql+"'"):""}
${has(wheresql)?(" and wheresql='"+wheresql+"'"):""}
${has(dataJurisdictionSql)?(dataJurisdictionSql):""}
ORDER BY  pa.`attributionName`, pp.`delayBeginDate` asc</Sql><Description>SELECT  
    pa.`guarantyOrgID`,
    pa.`guarantyOrgName`,
     pa.`oprationCompanyName`,
     pa.oprationCompanyID,
     pa.`hostAreaName`,
     pa.`attributionName`, 
     pp.projectName,
     pp.loadSum - IFNULL(tab1.payCapitalSum,0)-IFNULL(tab2.replaceCapitalSum,0)-IFNULL(tab5.creditorsum,0) AS `guarantySum`, 
     pp.`bankName`,
     pp.`subBankName`,
     pa.`fundSource`,
     pp.`delayBeginDate`,
     pp.`delayEndDate`,
     pp.`InterestMethodName`,
     pad.`dcontractCode`,
     pp.`amanName`,
     pp.`bmanName`,
     pp.beforeAManName,
     pp.beforeBManName,
     cc.relationMainName,
     cc.`clientGUID`,
     pp.bankRate,
     pp.serviceRate,
     pp.punishRate,
     pp.`project_ID`,
    tab3.disposeRemark  
 FROM pro_project pp
 LEFT JOIN pro_apply pa ON pa.`apply_ID`=pp.`apply_ID`
 LEFT JOIN crm_client cc ON cc.`client_ID`=pa.`client_ID`
 LEFT JOIN pro_applydetail pad ON pad.`applyDetail_ID` = pp.`applyDetail_ID`
 LEFT JOIN (SELECT pfp.project_ID, SUM(IFNULL(pfp.payCapitalSum,0)) AS payCapitalSum FROM pro_factpay pfp
    WHERE 1=1 ${has(endyearmonthsql)?(" and  DATE_FORMAT(pfp.payDate,'%Y-%m')&lt;='"+endyearmonthsql+"'"):""} GROUP BY pfp.project_ID
  )tab1 ON tab1.project_ID =  pp.project_ID
 LEFT JOIN (SELECT pre.project_ID,SUM(IFNULL(pre.replaceCapitalSum,0)) AS replaceCapitalSum FROM pro_replace pre
    WHERE 1=1 ${has(endyearmonthsql)?(" and  DATE_FORMAT(pre.replaceDate,'%Y-%m')&lt;='"+endyearmonthsql+"'"):""} GROUP BY pre.project_ID
   )tab2 ON tab2.project_ID =  pp.project_ID 
 LEFT JOIN (SELECT precre.project_ID,SUM(IFNULL(precre.creditor_sum,0)) AS creditorsum FROM pro_creditor precre
   WHERE 1=1 ${has(endyearmonthsql)?(" and precre.creditor_sum &lt;='"+endyearmonthsql+"'"):""} GROUP BY precre.project_ID
   )tab5 ON tab5.project_ID =  pp.project_ID 
 LEFT JOIN (SELECT CONCAT(po.disposeRemark) AS disposeRemark,po.apply_ID FROM pro_optguaranty po WHERE 1=1 GROUP BY po.apply_ID) tab3 ON tab3.apply_ID = pp.`apply_ID`
 WHERE pp.`busiClass`='02' 
-- AND pp.isFree = 0  原条件
 AND ( pp.finishDate is  null  ${has(endyearmonthsql)?(" or DATE_FORMAT(pp.finishDate,'%Y-%m') &gt; '"+endyearmonthsql+"'"):""}  )
${has(endyearmonthsql)?(" and DATE_FORMAT(pp.reportDate,'%Y-%m') &lt;= '"+endyearmonthsql+"'"):""}
${has(wheresql)?(" and wheresql='"+wheresql+"'"):""}
${has(dataJurisdictionSql)?(dataJurisdictionSql):""}
