LEFT JOIN pro_apply pa ON pp.apply_ID = pa.apply_ID
LEFT JOIN crm_client cc ON cc.`clientGUID` = pa.`clientGUID`
WHERE pp.busiClass='01'  AND cc.`natureID` = 'bb28e0f8b8bf47f787eb9d3d15de2b30'
${has(endyearmonthsql)?(" and DATE_FORMAT(pp.reportDate,'%Y-%m')&lt;='"+endyearmonthsql+"'"):""}
${has(wheresql)?(" and wheresql='"+wheresql+"'"):""}
${has(dataJurisdictionSql)?(dataJurisdictionSql):""}
GROUP BY pa.clientGUID ORDER BY cc.`clientName` ASC</Sql><Description>SELECT cc.`clientName`,SUM(pp.loadSum) loadSum 
