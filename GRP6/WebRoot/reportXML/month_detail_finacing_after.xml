ll></Item46></Children><ColumnWidths>60,80,80,80,80,267,121,100,136,80,99,112,91,96,94,110,150,136,108,139,108,60,198,72,81</ColumnWidths><RowHeights>30,30,30,30,30</RowHeights></Item0></Children><Type>102</Type></Item2></Children><PageFormat><Width>2808</Width><Height>834</Height><Left>15</Left><Top>18</Top><Right>6</Right><Bottom>18</Bottom></PageFormat></Page><NodeSource><Children ItemClass="DatasetNodeSource"><Item0><Children ItemClass="GroupNodeSource"><Item0><Children ItemClass="GroupNodeSource"><Item0><Children ItemClass="RowNodeSource"><Item0/></Children><Group><Field>attributionID</Field></Group></Item0></Children><Group><Field>oprationCompanyID</Field></Group></Item0></Children><Reader Class="SqlReader"><Name>tab_detail_factload</Name><Sql>SELECT  pa.`guarantyOrgName`,
     pa.`oprationCompanyName`,
     pa.oprationCompanyID,
     pa.`attributionID`,
     pa.`guarantyOrgID`,
     pa.`hostAreaName`,
     pa.`attributionName`, 
     pp.projectName,
     pp.loadSum - IFNULL(tab1.payCapitalSum,0)-IFNULL(tab2.replaceCapitalSum,0)-IFNULL(tab4.creditorsum,0) AS guarantySum,
     pp.`bankName`,
     pp.`periodMonthDay`,	
     pp.`busiTypeName`,
     pp.`subBankName`,
     pa.`fundSource`,
     pa.fundName,
     pa.fundType,
     pa.fundChinese,
     pp.`delayBeginDate`,
     pp.`delayEndDate`,
     pp.`overSum`,
     pp.`overDesc`,
     pp.`amanName`,
     cc.relationMainName,
     pp.`bmanName`,
     cc.`fullTradeName`,
     cc.`clientGUID`,
    -- pa.`clientGUID`,
     pp.beforeAManName,
     pp.beforeBManName,
     tab3.disposeRemark AS disposeRemark,
     tab3.fcValue,
     tab3.tdValue,
     tab3.jqValue,
     tab3.gqValue,
     tab3.optValue,
     pp.`project_ID`,

    (CASE 
     WHEN TIMESTAMPDIFF(MONTH,pp.loadBeginDate ,pp.delayEndDate )/12 &lt;0.5 then 1
     WHEN TIMESTAMPDIFF(MONTH,pp.loadBeginDate ,pp.delayEndDate )/12 &lt;1 and TIMESTAMPDIFF(MONTH,pp.loadBeginDate ,pp.delayEndDate )/12 &gt;=0.5  then 2
     WHEN TIMESTAMPDIFF(MONTH,pp.loadBeginDate ,pp.delayEndDate )/12 &lt;3 and TIMESTAMPDIFF(MONTH,pp.loadBeginDate ,pp.delayEndDate )/12 &gt;=1 then 3
     WHEN TIMESTAMPDIFF(MONTH,pp.loadBeginDate ,pp.delayEndDate )/12 &lt;5 and TIMESTAMPDIFF(MONTH,pp.loadBeginDate ,pp.delayEndDate )/12 &gt;=3 then 4
     WHEN TIMESTAMPDIFF(MONTH,pp.loadBeginDate ,pp.delayEndDate )/12 &gt;=5  then 5
     END) as type
 FROM pro_project pp
 LEFT JOIN (SELECT pfp.project_ID, SUM(IFNULL(pfp.payCapitalSum,0)) AS payCapitalSum FROM pro_factpay pfp
    WHERE 1=1 ${has(endyearmonthsql)?(" and  DATE_FORMAT(pfp.payDate,'%Y-%m')&lt;='"+endyearmonthsql+"'"):""} GROUP BY pfp.project_ID
  )tab1 ON tab1.project_ID =  pp.project_ID
 LEFT JOIN (SELECT pre.project_ID,SUM(IFNULL(pre.replaceCapitalSum,0)) AS replaceCapitalSum FROM pro_replace pre
    WHERE 1=1 ${has(endyearmonthsql)?(" and  DATE_FORMAT(pre.replaceDate,'%Y-%m')&lt;='"+endyearmonthsql+"'"):""} GROUP BY pre.project_ID
   )tab2 ON tab2.project_ID =  pp.project_ID 
LEFT JOIN (SELECT GROUP_CONCAT(po.disposeRemark) disposeRemark,po.apply_ID,
  CASE WHEN optName = '土地' THEN SUM(IFNULL(po.optValue,0)) END AS tdValue,
  CASE WHEN optName = '房产' THEN SUM(IFNULL(po.optValue,0)) END AS fcValue,
  CASE WHEN optName = '机器设备' THEN SUM(IFNULL(po.optValue,0)) END AS jqValue,
  CASE WHEN optTypeName = '股权' THEN SUM(IFNULL(po.optValue,0)) END AS gqValue,
                                SUM(IFNULL(po.optValue,0)) AS optValue FROM pro_optguaranty po 
 WHERE 1=1 GROUP BY po.apply_ID) tab3 ON tab3.apply_ID = pp.`apply_ID`
 LEFT JOIN (SELECT precre.project_ID,SUM(IFNULL(precre.creditor_sum,0)) AS creditorsum FROM pro_creditor precre
    WHERE 1=1 ${has(endyearmonthsql)?(" and  DATE_FORMAT(precre.creditor_date,'%Y-%m')&lt;='"+endyearmonthsql+"'"):""} GROUP BY precre.project_ID
   )tab4 ON tab4.project_ID =  pp.project_ID 
 LEFT JOIN pro_apply pa ON pa.`apply_ID`=pp.`apply_ID`
 LEFT JOIN crm_client cc ON cc.`client_ID`=pa.`client_ID`
 WHERE pp.`busiClass`='01' 
              AND ( pp.finishDate is  null  ${has(endyearmonthsql)?(" or DATE_FORMAT(pp.finishDate,'%Y-%m') &gt; '"+endyearmonthsql+"'"):""}  )
${has(endyearmonthsql)?(" and DATE_FORMAT(pp.reportDate,'%Y-%m') &lt;= '"+endyearmonthsql+"'"):""}
${has(wheresql)?(wheresql):""}
${has(dataJurisdictionSql)?(dataJurisdictionSql):""}
ORDER BY  pa.`attributionName`,pp.`delayBeginDate`</Sql><Description>SELECT  pa.`guarantyOrgName`,
