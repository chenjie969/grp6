     pa.`oprationCompanyName`,
     pa.`guarantyOrgID`,
     pa.`hostAreaName`,
     pa.`attributionName`, 
     pp.projectName,
     pp.loadSum,
     pp.`bankName`,
     pp.`subBankName`,
     pa.`fundName`,
     pa.fundChinese,
     pp.`delayBeginDate`,
     pp.`delayEndDate`,
     pp.`periodMonthDay`,
     pad.`busiTypeName`,
     pad.`dcontractCode`,
     pad.`jcontractCode`,
     pad.`bcontractCode`,
     pp.bankRate,
     pp.bzScale,
     pp.guarantyRate ,
     pp.serviceRate,
     pp.reviewRate ,
     pp.`amanName`,
     pp.`bmanName`,
     pp.beforeAManName,
     pp.beforeBManName,
     case when pp.`isNewClient`=1 then '是'
     else '是' end as isNewClient,
     -- if( pp.`isNewClient`=1,'是','否') as isNewClient,
     tab3.disposeRemark,
     cc.relationMainName  
 FROM pro_project pp 
 LEFT JOIN (SELECT CONCAT(po.disposeRemark) AS disposeRemark,po.apply_ID FROM pro_optguaranty po 
 WHERE 1=1 GROUP BY po.apply_ID) tab3 ON tab3.apply_ID = pp.`apply_ID`
 LEFT JOIN pro_apply pa ON pa.`apply_ID`=pp.`apply_ID`
 LEFT JOIN crm_client cc ON cc.`client_ID`=pa.`client_ID`
 LEFT JOIN pro_applydetail pad ON pad.`applyDetail_ID` = pp.`applyDetail_ID`
 WHERE pp.`busiClass`='01' and pp.isCreditorMark!=1
-- 统计时间由项目起始日期改为项目上报日期 
${has(begindatesql)?(" and pp.reportDate &gt;='"+begindatesql+"'"):""} 
${has(enddatesql)?(" and pp.reportDate&lt;='"+enddatesql+"'"):""}
${has(wheresql)?(" and wheresql='"+wheresql+"'"):""}
${has(dataJurisdictionSql)?(dataJurisdictionSql):""}
ORDER BY pa.`attributionName`,pp.`delayBeginDate` DESC</Sql><Description>SELECT  pa.`guarantyOrgName`,
