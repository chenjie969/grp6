<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%>
<div class="modal fade" id="InventoryView" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" data-backdrop="static" data-keyboard="false">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title" id="myModalLabel">查看存货</h4>
      </div>
      <div class="modal-body">
		<div class="row col-sm-offset-1">
			<h5 class="col-sm-11">存货名称：<span class="grey ztb_view_inventoryName"></span></h5>
			<div class="space-4"></div>
			<h5 class="col-sm-11">存货数量：<span class="grey ztb_view_inventoryCount"></span></h5>
			<div class="space-4"></div>
			<h5 class="col-sm-11">目前市场单价：<span class="grey ztb_view_inventoryPrice"></span>&nbsp;万元</h5>
			<div class="space-4"></div>
			<h5 class="col-sm-11">存货金额：<span class="grey ztb_view_inventorySum"></span>&nbsp;万元</h5>
			<div class="space-4"></div>
			<h5 class="col-sm-11">备注：<span class="grey ztb_view_remark"></span></h5>
		</div>			
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal"> <i class='icon-remove bigger-110'></i>关闭</button>
      </div>
    </div>
  </div>
</div>
				