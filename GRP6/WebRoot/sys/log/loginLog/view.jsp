<%@ page language="java" import="java.util.*" pageEncoding="UTF-8"%>
<div class="modal fade bs-example-modal-sm" id="viewloginLog" tabindex="-1" role="dialog" aria-labelledby="myModalLabel">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title" id="myModalLabel">查看上机日志</h4>
      </div>
      <div class="modal-body">
		<div class="row">
		   <div class="col-sm-offset-1">
				<!-- <h5 class="col-sm-12">担保机构编号：<span class="grey ztb_view_unitUid"></span></h5> -->
				<h5 class="col-sm-12">机构名称：<span class="grey ztb_view_unit_uidName"></span></h5>
				<h5 class="col-sm-12">上机时间：<span class="grey ztb_view_logindatetime" id="logindatetime"></span></h5>
				<h5 class="col-sm-12">上机人姓名：<span class="grey ztb_view_username"></span></h5>
				<h5 class="col-sm-12">上机部门名称：<span class="grey ztb_view_departname"></span></h5>
				<h5 class="col-sm-12">操作类型：<span class="grey ztb_view_logtype"></span></h5>
				<h5 class="col-sm-12">操作描述：<span class="grey ztb_view_logdescr"></span></h5>
				<h5 class="col-sm-12">登录账号：<span class="grey ztb_view_loginaccount"></span></h5>
				<h5 class="col-sm-12">ip地址：<span class="grey ztb_view_ipaddress"></span></h5>
			</div>
		</div>
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal"> <i class='icon-remove bigger-110'></i>关闭</button>
      </div>
    </div>
  </div>
</div>
